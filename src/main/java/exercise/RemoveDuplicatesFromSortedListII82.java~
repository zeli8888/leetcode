package exercise;

/**
 * @Author : Ze Li
 * @Date : 31/07/2025 13:56
 * @Version : V1.0
 * @Description :
 */
public class RemoveDuplicatesFromSortedListII82 {
    class Solution {
        public ListNode deleteDuplicates(ListNode head) {
            if (head == null) return null;
            ListNode root = new ListNode();
            ListNode[] lastTwoNodes = new ListNode[2];
            lastTwoNodes[0] = root;
            lastTwoNodes[1] = head;
            int lastVal = head.val;
            head = head.next;
            while (head != null) {
                if (head.val != lastVal) {
                    lastTwoNodes[0].next = lastTwoNodes[1];
                    lastTwoNodes[0] = lastTwoNodes[1];
                    lastTwoNodes[1] = head;
                    lastVal = head.val;
                }else {
                    lastTwoNodes[0].next = null;
                }
                head = head.next;
            }
            return root;
        }
    }
}
