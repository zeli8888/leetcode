package exercise;

/**
 * @Author : Ze Li
 * @Date : 12/03/2025 20:51
 * @Version : V1.0
 * @Description :
 */
public class FindMinimumInRotatedSortedArray153 {
    public int findMin(int[] nums) {
        int n = nums.length;
        int left = 0;
        int right = n - 1;
        int min = 5000;
        while (left < right){
            int mid = left + (right - left) / 2;
            if (nums[left] < nums[mid]){
                min = Math.min(nums[left], min);
                left = mid+1;
            }else {
                min = Math.min(nums[mid], min);
                right = mid;
            }
        }
        return min;
    }
}
